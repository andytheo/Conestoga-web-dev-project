        <img  class="logo-image" src="/images/logo.png" alt="logo-image">
        <div class="logo-text">Storeez</div>
    </a>
    <!-- Main menu -->
    <ul class="links">
        <li class="link"><a href="/">Home</a></li>
        <li class="link parent-link">
